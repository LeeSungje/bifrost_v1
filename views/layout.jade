      // Start Header Top Area
      .header-top-area
        .container
          .row
            .col-lg-4.col-md-4.col-sm-12.col-xs-12
              .logo-area
                a(href='/')
                  img(src='/img/logo/logo.png', alt='')
                span#currentTimeSpan(style='margin-right: 30px')   
            .col-lg-4.col-md-4.col-sm-12.col-xs-12
              .header-top-menu.align-middle
                button.btn.waves-effect.mode-slt-btn#modeSimulation(data-toggle="modal", data-target="#sktSimulModal") RM Simulation
                //button.btn.waves-effect.mode-slt-btn#btnDetailsView(data-toggle="modal", data-target="#sktViewModal") 상면 View
                #sktSimulModal.modal.fade(role="dialog", style="display:none;")
                  .modal-dialog.modals-default
                    .modal-content
                      .modal-header
                        button(type="button", data-dismiss="modal").close &times
                      .modal-body
                        h1.modal-title 장비실 선택
                        p.modal-sub-title 
                        .row
                          .skt-smr-modal-tbl
                            .col-lg-3.col-md-4.col-sm-6.col-xs-12.skt-modal-name-container
                              p.skt-smr-modal-txt.skt-smr-modal-tbl-hd 사옥명
                            .col-lg-2.col-md-2.col-sm-6.col-xs-12.skt-modal-floor-container
                              p.skt-smr-modal-txt.skt-smr-modal-tbl-hd 층수
                            .col-lg-3.col-md-4.col-sm-6.col-xs-12.skt-modal-location-container
                              p.skt-smr-modal-txt.skt-smr-modal-tbl-hd 장비실 구분
                            .col-lg-4.col-md-6.col-sm-6.col-xs-12.skt-modal-button-container
                              p.skt-smr-modal-txt.skt-smr-modal-tbl-hd 클릭하여 상세보기
                      .modal-footer
                #sktViewModal.modal.fade(role="dialog", style="display:none;")
                  .modal-dialog.modals-default
                    .modal-content
                      .modal-header
                        button(type="button", data-dismiss="modal").close &times
                      .modal-body
                        h1.modal-title 국사 선택 // RM 시뮬레이션 
                        p.modal-sub-title
                        .row
                          .skt-smr-modal-tbl
                            .col-lg-3.col-md-4.col-sm-6.col-xs-12.skt-modal-name-container
                              p.skt-smr-modal-txt.skt-smr-modal-tbl-hd 사옥명
                            .col-lg-2.col-md-2.col-sm-6.col-xs-12.skt-modal-floor-container
                              p.skt-smr-modal-txt.skt-smr-modal-tbl-hd 층수
                            .col-lg-3.col-md-4.col-sm-6.col-xs-12.skt-modal-location-container
                              p.skt-smr-modal-txt.skt-smr-modal-tbl-hd 장비실 구분
                            .col-lg-4.col-md-6.col-sm-6.col-xs-12.skt-modal-button-container
                              p.skt-smr-modal-txt.skt-smr-modal-tbl-hd 클릭하여 상세보기
                      .modal-footer
                if floor
                  button.btn.waves-effect.mode-slt-btn.info-btn 층수 : #{floor}F
                ul.nav.navbar-nav.notika-top-nav
            .col-lg-1.col-md-1.col-sm-4.col-xs-4
              .dropdown-trig-sgn
                button#selectedTeamName.btn.triger-fadeIn.waves-effect.team-name-btn(data-toggle="dropdown", aria-expended="false")
                  span#selectedTeamNameSpan
                  span &nbsp
                  span
                    i.fa.fa-caret-down
                ul#dropdown-centers.dropdown-menu.triger-fadeIn-dp
            .col-lg-1.col-md-2.col-sm-8.col-xs-4
              a(float='right')
                  img#speaker.align-middle(src='/speaker2.png', onclick='changeStatus()', width='50px', style='margin-top:5px') 
            .col-lg-2.col-md-2.col-sm-8.col-xs-4
              .header-top-menu
                button.btn.waves-effect.data-rcv-info-btn.data-rcv-info-ttl 미수집 : 
                button#data-rcv-info.btn.waves-effect.data-rcv-info-btn.btn-status(data-toggle="modal", data-target="#sktCollectionModal")
                //-  
                  span#vol-mute-btn.fa.fa-stack(data-mute="yes", onclick="muteAndUnmute()")
                    i.fa.fa-stack-2x.fa-volume-down
                    i.mute-on-off.fa.fa-stack-2x /
                #sktCollectionModal.modal.fade(role="dialog", style="display:none;")
                  .modal-dialog.modals-default
                    .modal-content
                      .modal-header
                        button(type="button", data-dismiss="modal").close &times
                      .modal-body
                        h1.modal-title 미수집 서버 목록
                        p.modal-sub-title &nbsp
                          button.btn.waves-effect.skt-collection-modal-mask-all(onclick="unMaskAllCollections()") Unmask All
                          span &nbsp&nbsp&nbsp
                          button.btn.waves-effect.skt-collection-modal-mask-all(onclick="maskAllCollections()") Mask All
                        p.modal-sub-title
                        .row
                          .skt-smr-modal-tbl
                            .col-lg-2.col-md-4.col-sm-8.col-xs-10.skt-modal-system-container
                              p.skt-smr-modal-txt.skt-smr-modal-tbl-hd.align-middle 시스템명
                            .col-lg-1.col-md-2.col-sm-4.col-xs-10.skt-modal-type-container
                              p.skt-smr-modal-txt.skt-smr-modal-tbl-hd.align-middle 시스템 타입
                            .col-lg-1.col-md-2.col-sm-4.col-xs-10.skt-modal-location-container
                              p.skt-smr-modal-txt.skt-smr-modal-tbl-hd.align-middle 사옥
                            .col-lg-1.col-md-2.col-sm-4.col-xs-4.skt-modal-building-container
                              p.skt-smr-modal-txt.skt-smr-modal-tbl-hd.align-middle 층수
                            .col-lg-1.col-md-2.col-sm-4.col-xs-4.skt-modal-floor-container
                              p.skt-smr-modal-txt.skt-smr-modal-tbl-hd.align-middle 상면
                            .col-lg-4.col-md-8.col-sm-12.col-xs-12.skt-modal-last-container
                              p.skt-smr-modal-txt.skt-smr-modal-tbl-hd.align-middle Last Update
                            .col-lg-2.col-md-2.col-sm-4.col-xs-4.skt-modal-mask-container
                              p.skt-smr-modal-txt.skt-smr-modal-tbl-hd.align-middle Mask
                            //.col-lg-3.col-md-8.col-sm-12.col-xs-12.skt-modal-last-container
                              p.skt-smr-modal-txt.skt-smr-modal-tbl-hd Last Update
                            //.col-lg-1.col-md-2.col-sm-4.col-xs-4.skt-modal-mask-container
                              p.skt-smr-modal-txt.skt-smr-modal-tbl-hd Mask
                            //.col-lg-2.col-md-6.col-sm-8.col-xs-8.skt-modal-button-container
                              p.skt-smr-modal-txt.skt-smr-modal-tbl-hd 클릭하여 상세보기
                              
                              


                
                              
